<div id="sketchpad">
  <div id="toolbar">
    <div class="tool freehand" onclick="Sketchpad.useTool('freehand');">F</div>
    <div class="tool line" onclick="Sketchpad.useTool('line');">L</div>
    <div class="tool rectangle" onclick="Sketchpad.useTool('rectangle');">R</div>
    <div class="tool ellipse" onclick="Sketchpad.useTool('ellipse');">E</div>
    <div class="tool undo" onclick="Sketchpad.undo();">U</div>
  </div>
  <div id="canvas"></div>
</div>
<div id="scoreboard">
</div>